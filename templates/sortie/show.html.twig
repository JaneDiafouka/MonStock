<!DOCTYPE html>

<title>Sortie</title>

{% block body %}
    <h1>Sortie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sortie.id }}</td>
            </tr>
            <tr>
                <th>Qtite</th>
                <td>{{ sortie.qtite }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ sortie.prix }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ sortie.date ? sortie.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_sortie_index') }}">back to list</a>

    <a href="{{ path('app_sortie_edit', {'id': sortie.id}) }}">edit</a>

    {{ include('sortie/_delete_form.html.twig') }}
{% endblock %}
